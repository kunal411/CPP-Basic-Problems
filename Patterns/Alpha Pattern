 /*
 A
 BB
 CCC
 */
 
 #include<iostream>
using namespace std;

int main(){

    int n;
    cin >> n;
    int i = 1;

    while (i <= n){

        int j = 1;
        while (j <= i){
            char var = 64 + i;
            cout << var;
            j = j + 1;
        }
        i = i + 1;
        cout << endl;
    }

}
