/*
A
BC
CDE
DEFG
*/

#include<iostream>
using namespace std;

int main(){

    int n;
    cin >> n;

    int i = 1;
    while (i <= n){

        int j = 1;
        while (j <= i){
            char c = 64 + i + j - 1;
            cout << c;
            j = j + 1;
        }
        i = i + 1;
        cout << endl;
    }

}
